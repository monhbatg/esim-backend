# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/esim_db

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here-make-it-long-and-random

# Application Configuration
NODE_ENV=development
PORT=3000
# CORS Origins (comma-separated list of allowed origins)
# For production, include all domains that will access the API
# Example: "https://www.goysim.mn,https://goysim.mn,https://app.goysim.mn"
CORS_ORIGINS=https://www.goysim.mn,https://goysim.mn

# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# eSIM API Configuration
ESIM_ACCESS_CODE=your-esim-access-code-here
